<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".seting">

    <TextView
        android:id="@+id/tvuseremail"
        android:layout_width="@dimen/widthmaintxta"
        android:layout_height="@dimen/hightmaintxta"
        android:layout_marginTop="292dp"
        android:text="email"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imag"
        android:layout_width="106dp"
        android:layout_height="99dp"
        android:layout_marginTop="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/user" />

    <Button
        android:id="@+id/settingbttna"
        android:layout_width="@dimen/bttnwidth"
        android:layout_height="@dimen/bttnhight"
        android:layout_marginTop="364dp"
        android:background="@color/lighblue"
        android:textSize="8pt"
        android:text="Contact us"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/settingbttnab"
        android:layout_width="@dimen/bttnwidth"
        android:layout_height="@dimen/bttnhight"
        android:layout_marginTop="464dp"
        android:background="@color/lighblue"
        android:text="ABOUT US"
        android:textSize="8pt"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="@dimen/textwidth"
        android:layout_height="@dimen/texthight"
        android:layout_marginTop="108dp"
        android:text="USER PROFILE"
        android:textAlignment="center"
        android:textColor="@color/middle_blue"
        android:textSize="10pt"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="351dp"
        android:layout_height="78dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nin" />

    <Button
        android:id="@+id/bttnsi"
        android:layout_width="@dimen/bttnwidth"
        android:layout_height="@dimen/bttnhight"
        android:layout_marginTop="36dp"
        android:backgroundTint="@color/red"
        android:text="Log Out"
        android:textColor="@color/white"
        android:textSize="9pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/settingbttnab" />

</androidx.constraintlayout.widget.ConstraintLayout>